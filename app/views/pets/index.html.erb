<h1>All Pets for <%= @owner.email %></h1>
<hr>
<% @owner.pets.each do |pet| %>
  <p>
    <%= link_to pet.name, owner_pet_path(@owner, pet) %>
    <br>
    (<%= pet.breed %>)
  </p>
<% end %>
<hr>
<%= link_to "Add another pet", new_owner_pet_path(@owner) %>
<br>
<%= link_to "View all owners", owner_path(@owner) %>
